{
  "name": "google-genai-api",
  "version": "1.2.2",
  "description": "Node.js wrapper for Google GenAI API (Gemini, Imagen 4, and Veo 3.1 for image and video generation)",
  "type": "module",
  "main": "api.js",
  "bin": {
    "google-genai": "./cli.js"
  },
  "exports": {
    ".": "./api.js",
    "./api": "./api.js",
    "./video": "./video-api.js",
    "./veo": "./veo-api.js",
    "./utils": "./utils.js",
    "./config": "./config.js"
  },
  "files": [
    "api.js",
    "video-api.js",
    "veo-api.js",
    "cli.js",
    "utils.js",
    "config.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "google": "node cli.js",
    "google:help": "node cli.js --help",
    "google:examples": "node cli.js --examples",
    "google:gemini": "node cli.js --gemini",
    "google:imagen": "node cli.js --imagen",
    "google:veo": "node cli.js --veo",
    "google:veo:fast": "node cli.js --veo --veo-model veo-3.1-fast-generate-preview",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "google",
    "genai",
    "gemini",
    "imagen",
    "veo",
    "ai",
    "image-generation",
    "video-generation",
    "text-to-image",
    "text-to-video",
    "image-to-image",
    "image-to-video",
    "semantic-masking"
  ],
  "author": "img-gen Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aself101/google-genai-api.git"
  },
  "bugs": {
    "url": "https://github.com/aself101/google-genai-api/issues"
  },
  "homepage": "https://github.com/aself101/google-genai-api#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "file-type": "^19.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "semantic-release": "^24.2.9",
    "vitest": "^1.1.0"
  }
}
