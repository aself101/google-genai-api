{
  "name": "google-genai-api",
  "version": "1.3.0",
  "description": "Node.js wrapper for Google GenAI API (Gemini, Imagen 4, and Veo 3.1 for image and video generation)",
  "type": "module",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "bin": {
    "google-genai": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./video": {
      "import": "./dist/video-api.js",
      "types": "./dist/video-api.d.ts"
    },
    "./veo": {
      "import": "./dist/veo-api.js",
      "types": "./dist/veo-api.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "types": "./dist/config.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "google": "tsx src/cli.ts",
    "google:help": "tsx src/cli.ts --help",
    "google:examples": "tsx src/cli.ts --examples",
    "google:gemini": "tsx src/cli.ts --gemini",
    "google:imagen": "tsx src/cli.ts --imagen",
    "google:veo": "tsx src/cli.ts --veo",
    "google:veo:fast": "tsx src/cli.ts --veo --veo-model veo-3.1-fast-generate-preview",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "google",
    "genai",
    "gemini",
    "imagen",
    "veo",
    "ai",
    "image-generation",
    "video-generation",
    "text-to-image",
    "text-to-video",
    "image-to-image",
    "image-to-video",
    "semantic-masking"
  ],
  "author": "img-gen Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aself101/google-genai-api.git"
  },
  "bugs": {
    "url": "https://github.com/aself101/google-genai-api/issues"
  },
  "homepage": "https://github.com/aself101/google-genai-api#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "file-type": "^19.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "semantic-release": "^24.2.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  }
}
